{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "Publishing date",
          "name": "date",
          "type": "datetime",
          "default": "{{now}}",
          "isPublishDate": true
        },
        {
          "title": "Last modified date",
          "name": "lastmod",
          "type": "datetime",
          "default": "{{now}}",
          "isModifiedDate": true
        },
        {
          "title": "Content preview",
          "name": "preview",
          "type": "image",
          "isPreviewImage": true
        },
        {
          "title": "Is in draft",
          "name": "isdraft",
          "type": "boolean"
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "Categories",
          "name": "categories",
          "type": "categories"
        },
        {
          "title": "Published",
          "name": "published",
          "type": "boolean"
        },
        {
          "title": "fmContentType",
          "name": "fmContentType",
          "type": "string",
          "default": "default"
        },
        {
          "title": "mermaid",
          "name": "mermaid",
          "type": "boolean",
          "default": "false"
        }
      ]
    },
    {
      "name": "posts",
      "pageBundle": false,
      "previewPath": null,
      "template": "[[workspace]]/.frontmatter/templates/post-template.md",
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "Publishing date",
          "name": "date",
          "type": "datetime",
          "default": "{{now}}",
          "isPublishDate": true
        },
        {
          "title": "Last modified date",
          "name": "lastmod",
          "type": "datetime",
          "default": "{{now}}",
          "isModifiedDate": true
        },
        {
          "title": "Content preview",
          "name": "preview",
          "type": "image",
          "isPreviewImage": true
        },
        {
          "title": "Is in isdraft",
          "name": "isdraft",
          "type": "boolean"
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "Categories",
          "name": "categories",
          "type": "categories"
        },
        {
          "title": "Published",
          "name": "published",
          "type": "boolean"
        },
        {
          "title": "Type",
          "name": "type",
          "type": "string",
          "default": "posts"
        },
        {
          "title": "layout",
          "name": "layout",
          "type": "string",
          "default": "posts"
        },
        {
          "title": "fmContentType",
          "name": "fmContentType",
          "type": "string",
          "default": "posts"
        },
        {
          "title": "mermaid",
          "name": "mermaid",
          "type": "boolean",
          "default": "false"
        }
      ]
    },
    {
      "name": "pages",
      "pageBundle": false,
      "previewPath": null,
      "defaultFileName": "{{title}}",
      "template": "[[workspace]]/.frontmatter/templates/page-template.md",
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "Publishing date",
          "name": "date",
          "type": "datetime",
          "default": "{{now}}",
          "isPublishDate": true
        },
        {
          "title": "Last modified date",
          "name": "lastmod",
          "type": "datetime",
          "default": "{{now}}",
          "isModifiedDate": true
        },
        {
          "title": "Content preview",
          "name": "preview",
          "type": "image",
          "isPreviewImage": true
        },
        {
          "title": "Is in isdraft",
          "name": "isdraft",
          "type": "boolean",
          "default": true
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "Categories",
          "name": "categories",
          "type": "categories"
        },
        {
          "title": "Published",
          "name": "published",
          "type": "boolean"
        },
        {
          "title": "Type",
          "name": "type",
          "type": "string",
          "default": "pages"
        },
        {
          "title": "layout",
          "name": "layout",
          "type": "string",
          "default": "pages"
        },
        {
          "title": "fmContentType",
          "name": "fmContentType",
          "type": "string",
          "default": "pages"
        },
        {
          "title": "mermaid",
          "name": "mermaid",
          "type": "boolean",
          "default": false
        }
      ]
    },
    {
      "name": "index",
      "pageBundle": false,
      "fields": [
        {
          "title": "title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "published",
          "name": "published",
          "type": "boolean"
        },
        {
          "title": "type",
          "name": "type",
          "type": "string"
        }
      ]
    }
  ],
  "frontMatter.framework.id": "jekyll",
  "frontMatter.content.publicFolder": {
    "path": "assets",
    "relative": false
  },
  "frontMatter.global.timezone": "Australia/Sydney",
  "frontMatter.preview.host": "http://localhost:4000",
  "frontMatter.sponsors.ai.enabled": false,
  "frontMatter.content.isdraftField": {
    "name": "isdraft",
    "type": "boolean"
  },
  "frontMatter.content.pageFolders": [
    {
      "title": "posts",
      "path": "[[workspace]]/_posts",
      "contentTypes": [
        "posts"
      ],
      "filePrefix": "{{year}}-{{month}}-{{day}}"
    },
    {
      "title": "pages",
      "path": "[[workspace]]/pages",
      "contentTypes": [
        "pages"
      ],
      "filePrefix": ""
    }
  ],
  "frontMatter.git.enabled": true,
  "frontMatter.website.host": "https://tlourey.github.io",
  "frontMatter.dashboard.openOnStart": true,
  "frontMatter.dashboard.content.pagination": 30,
  "frontMatter.content.autoUpdateDate": true,
  "frontMatter.data.folders": [
    {
      "id": "data",
      "path": "[[workspace]]/data",
      "singleEntry": true,
      "enableFileCreation": true,
      "fileType": "json",
      "schema": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "title": "Name"
          },
          "url": {
            "type": "string",
            "title": "URL"
          },
          "description": {
            "type": "string",
            "title": "Description"
          }
        }
      }
    }
  ],
  "frontMatter.snippets.wrapper.enabled": false,
  "frontMatter.content.snippets": {
    "VSCodeExtension": {
      "description": "For headings about VSCode Extensions",
      "body": [
        "",
        "## [[ExtensionName]]",
        "",
        "<https://marketplace.visualstudio.com/items?itemName=[[VSCodeSlug]]>\\",
        "<vscode:extension/[[VSCodeSlug]]>",
        "",
        "### [[ExtensionName]] Commands",
        "",
        "### [[ExtensionName]] Notes",
        "",
        "### [[ExtensionName]] Tips",
        "",
        "### [[ExtensionName]] References",
        ""
      ],
      "fields": [
        {
          "name": "ExtensionName",
          "title": "Extension Name",
          "type": "string"
        },
        {
          "name": "VSCodeSlug",
          "title": "VSCode Slug",
          "type": "string"
        },
        {
          "name": "VSCodeSlug",
          "title": "VSCode Slug",
          "type": "string"
        },
        {
          "name": "ExtensionName",
          "title": "Extension Name",
          "type": "string"
        },
        {
          "name": "ExtensionName",
          "title": "Extension Name",
          "type": "string"
        },
        {
          "name": "ExtensionName",
          "title": "Extension Name",
          "type": "string"
        },
        {
          "name": "ExtensionName",
          "title": "Extension Name",
          "type": "string"
        }
      ]
    },
    "ToolName": {
      "description": "For headings about tools",
      "body": [
        "",
        "## [[ToolName]]",
        "",
        "### [[ToolName]] Commands",
        "",
        "### [[ToolName]] Notes",
        "",
        "### [[ToolName]] Tips",
        "",
        "### [[ToolName]] References",
        ""
      ],
      "fields": [
        {
          "name": "ToolName",
          "title": "ToolName",
          "type": "string",
          "single": true,
          "default": ""
        },
        {
          "name": "ToolName",
          "title": "ToolName",
          "type": "string",
          "single": true,
          "default": ""
        },
        {
          "name": "ToolName",
          "title": "ToolName",
          "type": "string",
          "single": true,
          "default": ""
        },
        {
          "name": "ToolName",
          "title": "ToolName",
          "type": "string",
          "single": true,
          "default": ""
        },
        {
          "name": "ToolName",
          "title": "ToolName",
          "type": "string",
          "single": true,
          "default": ""
        }
      ]
    },
    "Image Snippet": {
      "description": "Image with caption",
      "body": "{{< caption \"[[&mediaUrl]]\" \"[[caption]]\" >}}",
      "isMediaSnippet": true
    },
    "cSpell: Ignore": {
      "description": "",
      "body": "<!--- cSpell:ignore [[Word]] --->",
      "fields": [
        {
          "name": "Word",
          "title": "Word",
          "type": "string",
          "single": true,
          "default": ""
        }
      ]
    },
    "cSpell: Words": {
