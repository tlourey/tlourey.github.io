{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "Publishing date",
          "name": "date",
          "type": "datetime",
          "default": "{{now}}",
          "isPublishDate": true
        },
        {
          "title": "Content preview",
          "name": "preview",
          "type": "image"
        },
        {
          "title": "Is in draft",
          "name": "draft",
          "type": "draft"
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "Categories",
          "name": "categories",
          "type": "categories"
        }
      ]
    },
    {
      "name": "posts",
      "pageBundle": false,
      "previewPath": null,
      "template": "[[workspace]]/.frontmatter/templates/post-template.md",
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "Publishing date",
          "name": "date",
          "type": "datetime",
          "default": "{{now}}",
          "isPublishDate": true
        },
        {
          "title": "Content preview",
          "name": "preview",
          "type": "image"
        },
        {
          "title": "Is in draft",
          "name": "draft",
          "type": "draft"
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "Categories",
          "name": "categories",
          "type": "categories"
        },
        {
          "title": "Published",
          "name": "published",
          "type": "boolean"
        },
        {
          "title": "Type",
          "name": "type",
          "type": "string",
          "default": "posts"
        }
      ]
    },
    {
      "name": "pages",
      "pageBundle": false,
      "previewPath": null,
      "defaultFileName": "{{title}}",
      "template": "[[workspace]]/.frontmatter/templates/page-template.md",
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "Is in draft",
          "name": "draft",
          "type": "draft"
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "Categories",
          "name": "categories",
          "type": "categories"
        },
        {
          "title": "Published",
          "name": "published",
          "type": "boolean"
        },
        {
          "title": "Type",
          "name": "type",
          "type": "string",
          "default": "pages"
        }
      ]
    }
  ],
  "frontMatter.framework.id": "jekyll",
  "frontMatter.content.publicFolder": "assets",
  "frontMatter.preview.host": "http://localhost:4000",
  "frontMatter.sponsors.ai.enabled": false,
  "frontMatter.content.pageFolders": [
    {
      "title": "posts",
      "path": "[[workspace]]/_posts",
      "contentTypes": [
        "posts"
      ],
      "filePrefix": "{{year}}-{{month}}-{{day}}"
    },
    {
      "title": "pages",
      "path": "[[workspace]]/pages",
      "contentTypes": [
        "pages"
      ],
      "filePrefix": ""
    }
  ],
  "frontMatter.git.enabled": true,
  "frontMatter.website.host": "https://tlourey.github.io",
  "frontMatter.dashboard.openOnStart": true
}