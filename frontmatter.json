          "default": ""
        },
        {
          "name": "Contexts",
          "title": "Contexts",
          "type": "string",
          "single": true,
          "default": ""
        },
        {
          "name": "My Notes",
          "title": "My Notes",
          "type": "string",
          "single": true,
          "default": ""
